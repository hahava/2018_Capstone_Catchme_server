<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.mapper-product">

	<select id="getlist" parameterType="String"
		resultType="capstone.sejong.chemical.ChemicalDTO">

		select * from cheminfo where nameK=#{key}

	</select>


	<select id="getgradient" parameterType="String"
		resultType="String">

		select productgradient
		from product where productname =
		#{productName}

	</select>

</mapper>