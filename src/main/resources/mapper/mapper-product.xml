<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.product">

	<select id="selectChemicalInfo" parameterType="string" resultType="capstone.sejong.chemical.ChemicalDTO">
		SELECT
			*
		FROM
			cheminfo
		WHERE
			nameK = #{'key'}
	</select>

	<select id="selectIngredientNames" parameterType="string" resultType="string">
		SELECT
			productingredient
		FROM
			product
		WHERE
			productname = #{'productName'}
	</select>

</mapper>